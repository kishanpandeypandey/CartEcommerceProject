<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>RegisterServlet</servlet-name>
  	<display-name>RegisterServlet</display-name>
  	<description></description>
  	<servlet-class>com.java.ecommerce.servlet.RegisterServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>loginServlet</servlet-name>
  	<display-name>loginServlet</display-name>
  	<description></description>
  	<servlet-class>com.java.ecommerce.servlet.loginServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>logoutservlet</servlet-name>
  	<display-name>logoutservlet</display-name>
  	<description></description>
  	<servlet-class>com.java.ecommerce.servlet.logoutservlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>CategoryOpreationServlet</servlet-name>
  	<display-name>CategoryOpreationServlet</display-name>
  	<description></description>
  	<servlet-class>com.java.ecommerce.servlet.CategoryOpreationServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ProductOpreationServlet</servlet-name>
  	<display-name>ProductOpreationServlet</display-name>
  	<description></description>
  	<servlet-class>com.java.ecommerce.servlet.ProductOpreationServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>RegisterServlet</servlet-name>
  	<url-pattern>/RegisterServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>loginServlet</servlet-name>
  	<url-pattern>/loginServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>logoutservlet</servlet-name>
  	<url-pattern>/logoutservlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>CategoryOpreationServlet</servlet-name>
  	<url-pattern>/CategoryOpreationServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ProductOpreationServlet</servlet-name>
  	<url-pattern>/ProductOpreationServlet</url-pattern>
  </servlet-mapping>
</web-app>
